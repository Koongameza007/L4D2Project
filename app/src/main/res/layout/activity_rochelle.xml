<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/_11"
    tools:context=".Rochelle">

    <Button
        android:id="@+id/backbt3"
        android:layout_width="132dp"
        android:layout_height="62dp"
        android:layout_marginTop="652dp"
        android:backgroundTint="#B80909"
        android:text="Back"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.942"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ScrollView
        android:layout_width="349dp"
        android:layout_height="440dp"
        android:layout_marginTop="144dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="348dp"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/_10" />

            <TextView
                android:id="@+id/textView10"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="โครเป็นผู้ผลิตรายย่อย บริษัท ร่วมสำหรับสถานีข่าวที่มีชื่อเสียงในระดับสูงแม้ว่างานของเธอมีมากขึ้นจะทำอย่างไรกับสายการย้ายและหน้าที่กาแฟ เธอถูกในสะวันนาการผลิตส่วนที่เกี่ยวกับศูนย์อพยพเมื่อพื้นที่กลายเป็นที่แพร่หลายกับที่ติดเชื้อ . เธอสวมเสื้อยืดสีชมพูพร้อมโหมด Depecheโลโก้, ต่างหูห่วงสีเงิน, สีฟ้ากางเกงยีนส์, เข็มขัดเรียงรายรอบเอวบางของเธอทั้งสองกำไลเงินบนแขนซ้ายของเธอและข้อเข่าสูงรองเท้าสีน้ำตาล โครมีความเข้าใจที่เหมือนจริงมากที่สุดของสถานการณ์และได้รับการดูแลที่ดีต่อการรอดชีวิตของเธอเพื่อน โครและฟรานซิสแสดงความสนใจที่โรแมนติกในแต่ละอื่น ๆ เมื่อพวกเขาตอบสนองในการรณรงค์ผ่าน. ในเกมแชลล์มีการตั้งค่าที่คล้ายกับนิค; เธอชอบปืนกลมือและปืนไรเฟิลจู่โจมสายพันธุ์ "
                android:textAlignment="center"
                android:textColor="#F8F8F8"
                android:textSize="18sp" />
        </LinearLayout>
    </ScrollView>

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="80dp"
        android:text="Rochelle"
        android:textColor="#FFFFFF"
        android:textSize="48sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>